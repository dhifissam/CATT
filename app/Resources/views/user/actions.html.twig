{% import ":layout/macros:buttons.html.twig" as buttons %}
{% if viewType == "actions" %}
    <ul class="icons-list">
        <li>{{ buttons.deleteAction(path("user_delete",{id:item.id})) }}</li>
        <li>{{ buttons.updateAction(path("user_form",{id:item.id})) }}</li>
    </ul>
    <script>$('[data-popup="tooltip"]').tooltip();</script>
{% endif %}
{% if viewType == "roles" %}
    {% for role in item.roles %}
        {% if role != "ROLE_USER" %}
            <p> - {{ role }}</p>
        {% endif %}
    {% endfor %}
{% endif %}
{% if viewType == "status" %}
    {% if item.enabled %}
        <span class="label label-success">Enbled</span>
    {% else %}
        <span class="label label-danger">Desabled</span>
    {% endif %}
{% endif %}